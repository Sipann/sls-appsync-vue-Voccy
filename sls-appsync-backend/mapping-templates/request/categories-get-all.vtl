{
  "version" : "2017-02-28",
  "operation" : "Query",
  "query" : {
    "expression" : "UserId = :userId AND begins_with(ItemId, :cat)",
    "expressionValues" : {
        ":userId" : $util.dynamodb.toDynamoDBJson($ctx.identity.sub),
        ":cat" : $util.dynamodb.toDynamoDBJson("CAT"),
    },
  },
  "select" : "ALL_ATTRIBUTES",
}